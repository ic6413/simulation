if "${ifpour} == yes && ${rst_from} == 0" then "jump SELF end_pour6_innerwall"
if "${ifstoreforce} == yes" then "fix	storeforce_inwall_before	all store/force"
if "${contact_model} != hooke_his" &
 then "jump SELF hooke_his_end1"

if "${ifrotate} == yes" then &
  "fix	inwall		all wall/gran hooke/history ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zcylinder ${ri_wall} shear x ${in_velocity}" &
else &
  "fix	inwall		all wall/gran hooke/history ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zcylinder ${ri_wall}"
label hooke_his_end1

if "${contact_model} != hooke" &
 then "jump SELF hooke_end1"
if "${ifrotate} == yes" then &
  "fix	inwall		all wall/gran hooke ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zcylinder ${ri_wall} shear x ${in_velocity}" &
else &
  "fix	inwall		all wall/gran hooke ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zcylinder ${ri_wall}"
label hooke_end1
if "${ifstoreforce} == yes" then "fix	storeforce_inwall_after	all store/force"
label	end_pour6_innerwall

if "${contact_model} != hooke_his" &
 then "jump SELF hooke_his_end2"
if "${ifstoreforce} == yes" then "fix	storeforce_outwall_before	all store/force"
fix	outwall 	all wall/gran hooke/history ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zcylinder ${ro_wall}
if "${ifstoreforce} == yes" then "fix	storeforce_outwall_after	all store/force"
if "${ifstoreforce} == yes" then "fix	storeforce_zbottom_before	all store/force"
fix	zplane_bottom 	all wall/gran hooke/history ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zplane ${zlo_wall} NULL
if "${ifstoreforce} == yes" then "fix	storeforce_zbottom_after	all store/force"
if "${ifztop} != yes" then "jump SELF ztop_end1"
if "${ifstoreforce} == yes" then "fix	storeforce_ztop_before	all store/force"
fix	zplane_top 	all wall/gran hooke/history ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zplane NULL ${zhi_wall}
if "${ifstoreforce} == yes" then "fix	storeforce_ztop_after	all store/force"
label ztop_end1
label hooke_his_end2

if "${contact_model} != hooke" &
 then "jump SELF hooke_end2"
if "${ifstoreforce} == yes" then "fix	storeforce_outwall_before	all store/force"
fix	outwall 	all wall/gran hooke ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zcylinder ${ro_wall}
if "${ifstoreforce} == yes" then "fix	storeforce_outwall_after	all store/force"
if "${ifstoreforce} == yes" then "fix	storeforce_zbottom_before	all store/force"
fix	zplane_bottom 	all wall/gran hooke ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zplane ${zlo_wall} NULL
if "${ifstoreforce} == yes" then "fix	storeforce_zbottom_after	all store/force"
if "${ifztop} != yes" then "jump SELF ztop_end2"
if "${ifstoreforce} == yes" then "fix	storeforce_ztop_before	all store/force"
fix	zplane_top 	all wall/gran hooke ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} zplane NULL ${zhi_wall}
if "${ifstoreforce} == yes" then "fix	storeforce_ztop_after	all store/force"
label ztop_end2
label hooke_end2


