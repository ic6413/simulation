## y_bottom force
# compute total force on particles before y_bottom apply 
compute		sum_storeforce_y_bottom_before_xyz all reduce sum f_storeforce_y_bottom_before[*]
# compute total force on particles after y_bottom apply 
compute		sum_storeforce_y_bottom_after_xyz all reduce sum f_storeforce_y_bottom_after[*]
# total force all atoms on y_bottom by substract
variable	force_y_bottom_x equal c_sum_storeforce_y_bottom_after_xyz[1]-c_sum_storeforce_y_bottom_before_xyz[1]
variable	force_y_bottom_y equal c_sum_storeforce_y_bottom_after_xyz[2]-c_sum_storeforce_y_bottom_before_xyz[2]
variable	force_y_bottom_z equal c_sum_storeforce_y_bottom_after_xyz[3]-c_sum_storeforce_y_bottom_before_xyz[3]

## y_top force
# compute total force on particles before y_top apply 
compute		sum_storeforce_y_top_before_xyz all reduce sum f_storeforce_y_top_before[*]
# compute total force on particles after y_top apply 
compute		sum_storeforce_y_top_after_xyz all reduce sum f_storeforce_y_top_after[*]
# total force all atoms on y_top by substract
variable	force_y_top_x equal c_sum_storeforce_y_top_after_xyz[1]-c_sum_storeforce_y_top_before_xyz[1]
variable	force_y_top_y equal c_sum_storeforce_y_top_after_xyz[2]-c_sum_storeforce_y_top_before_xyz[2]
variable	force_y_top_z equal c_sum_storeforce_y_top_after_xyz[3]-c_sum_storeforce_y_top_before_xyz[3]

## zbottom force
# compute total force on particles before zbottom apply 
compute		sum_storeforce_zbottom_before_xyz all reduce sum f_storeforce_zbottom_before[*]
# compute total force on particles after zbottom apply 
compute		sum_storeforce_zbottom_after_xyz all reduce sum f_storeforce_zbottom_after[*]
# total force all atoms on zbottom by substract
variable	force_zbottom_x equal c_sum_storeforce_zbottom_after_xyz[1]-c_sum_storeforce_zbottom_before_xyz[1]
variable	force_zbottom_y equal c_sum_storeforce_zbottom_after_xyz[2]-c_sum_storeforce_zbottom_before_xyz[2]
variable	force_zbottom_z equal c_sum_storeforce_zbottom_after_xyz[3]-c_sum_storeforce_zbottom_before_xyz[3]

