if "${contact_model} != hooke_his" &
 then "jump SELF hooke_his_end1"

if "${ifrotate} == yes" then &
  "fix	y_bottom		all wall/gran hooke/history ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} yplane ${ylo_wall} NULL shear x ${in_velocity}" &
else &
  "fix	y_bottom		all wall/gran hooke/history ${kn} ${kt} ${gamma_n} ${gamma_t} ${xmu} ${ifdamp_tangent} yplane ${ylo_wall} NULL"
